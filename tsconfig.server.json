{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "lib": ["es6", "dom"],
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "suppressImplicitAnyIndexErrors": true,
      "noUnusedLocals": true
    },
    "include": [
        "hello"
    ]
  }